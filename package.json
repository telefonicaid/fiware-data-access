{
  "name": "fda",
  "license": "AGPL-3.0-only",
  "version": "0.0.1",
  "description": "fiware-data-access",
  "homepage": "https://github.com/telefonicaid/fiware-data-access",
  "type": "module",
  "scripts": {
    "clean": "rm -rf package-lock.json && rm -rf node_modules",
    "dev": "nodemon fda.js",
    "lint": "eslint lib/ --cache",
    "lint:fix": "eslint  lib/ --cache --fix",
    "prettier": "prettier --config .prettierrc.json --write '**/**/**/**/*.js' '**/**/**/*.js' '**/**/*.js' '**/*.js' '*.js'",
    "start": "node .",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --runInBand"
  },
  "engines": {
    "node": ">=24"
  },
  "dependencies": {
    "@aws-sdk/lib-storage": "~3.934.0",
    "@duckdb/node-api": "~1.4.1-r.1",
    "express": "5.1.0",
    "mongodb": "~7.0.0",
    "pg": "~8.16.3",
    "pg-copy-streams": "~7.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.972.0",
    "ajv": "~8.17.1",
    "dotenv": "~17.2.3",
    "eslint": "~8.18.0",
    "eslint-config-tamia": "~8.0.0",
    "eslint-plugin-prettier": "~4.0.0",
    "globals": "~16.5.0",
    "husky": "~4.3.8",
    "jest": "~29.7.0",
    "jest-mock-extended": "4.0.0",
    "lint-staged": "~12.3.8",
    "lodash": "~4.17.21",
    "nodemon": "~3.1.10",
    "prettier": "~2.7.1",
    "supertest": "^7.2.2",
    "testcontainers": "^11.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --parser markdown --tab-width 4 --print-width 120 --write --prose-wrap always"
    ],
    "*.js": [
      "prettier --config .prettierrc.json --write"
    ]
  }
}
